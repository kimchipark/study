
public class Calculater 
{
	public void MakeSum(int max)
	{
		int i;
		
		int sum = 0 ;
		
		for( i = 1; i <= max; i++)
		{
			sum += i;
		}
		System.out.println(sum);
	}
	
	public void PrintStar(int max)
	{
		for(int i = 0 ; i < max; i++)
		{
			System.out.print("*");
		}
		System.out.println();
	}
}
