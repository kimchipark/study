
public class Calculater 
{
	public void MakeSum(int max)
	{
		int i;
		
		int sum = 0 ;
		
		for( i = 1; i <= max; i++)
		{
			sum += i;
		}
		System.out.println(sum);
	}
	
	public void PrintGugudan(int y)
	{
		for(int i = 1 ; i <= 9 ; i++)
		{
			System.out.println(y + "x" + i + "=" + (y*i));
		}
		System.out.println();
	}
}
